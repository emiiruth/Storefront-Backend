{
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "plugins": ["prettier"],
   "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended", 
    "plugin:@typescript-eslint/recommended"],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-explicit-any": ["error", { "ignoreRestArgs": true }],
      "@typescript-eslint/no-unused-vars": "error", 
      "prefer-const": "error"
    }
  }

